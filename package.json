{
  "name": "zui5_training",
  "version": "0.3.2",
  "private": true,
  "description": "A Fiori UI5 training application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^2.14.1",
    "nwabap-ui5uploader": "^1.2.0",
    "rimraf": "3.0.2"
  },
  "scripts": {
    "start": "fiori run --open 'test/flpSandbox.html#ui5trainingzui5training-display'",
    "start-local": "fiori run --config ./ui5-local.yaml --open 'test/flpSandbox.html#ui5trainingzui5training-display'",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "npx nwabap upload",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open 'index.html'",
    "start-variants-management": "fiori run --open \"preview.html?fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "unit-tests": "fiori run --open test/unit/unitTests.qunit.html",
    "int-tests": "fiori run --open test/integration/opaTests.qunit.html",
    "undeploy": "fiori undeploy --config ui5-deploy.yaml",
    "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling"
    ]
  },
  "sapuxLayer": "CUSTOMER_BASE",
  "dependencies": {
    "generator-easy-ui5": "^3.2.2"
  }
}
